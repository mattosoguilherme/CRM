// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Doctor {
  id                String @id @default(uuid())
  name              String
  crm               Int    @unique
  landline          Int
  cell_phone        Int
  cep               Int
  medical_specialty String

  @@map("doctor")
}
