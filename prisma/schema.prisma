generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Doctor {
  id                String   @id @default(uuid())
  name              String   @db.VarChar(120)
  crm               Int      @unique
  landline          Int
  cell_phone        Int
  medical_specialty String
  cep               Int
  logradouro        String
  bairro            String
  localidade        String
  uf                String
  complement        String
  createdAT         DateTime @default(now())
  updatedAT         DateTime @updatedAt()

  @@map("doctor")
}
